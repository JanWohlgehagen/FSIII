   <rowConstraints>
      <RowConstraints maxHeight="70.0" minHeight="0.0" prefHeight="58.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="444.0" minHeight="10.0" prefHeight="393.0" vgrow="SOMETIMES" />
      <RowConstraints maxHeight="139.0" minHeight="0.0" prefHeight="51.0" vgrow="SOMETIMES" />
